@using StudentSystem.Services.Data.Features.Courses.Constants
@model CourseFormBindingModel

@{
    this.ViewData["Title"] = "Update Course";
}

@section HeadScripts
{
    @{
        await Html.RenderPartialAsync("_TinymceScriptsPartial");
    }
}

<div class="container mt-5">
    @{
        await Html.RenderPartialAsync("_CourseFormPartial", new ViewDataDictionary(ViewData) { { CourseConstants.DisplayUploadNewImage, true } });
    }
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}